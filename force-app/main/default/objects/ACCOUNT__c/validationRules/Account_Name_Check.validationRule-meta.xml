<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Account_Name_Check</fullName>
    <active>true</active>
    <errorConditionFormula>IF(
     REGEX(  Account_Holder_Name__c , &apos;^[A-Za-z]+$&apos;) , 

    false, 
 
    true
)</errorConditionFormula>
    <errorDisplayField>Account_Holder_Name__c</errorDisplayField>
    <errorMessage>Account name can have only alphabetical character.</errorMessage>
</ValidationRule>
